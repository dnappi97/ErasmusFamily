<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.erasmusfamily">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Setting.ChangePasswordActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Setting.SettingActivity" />
        </activity>

        <activity android:name=".Setting.SettingActivity"></activity>

        <activity android:name=".Social.FormLogActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Social.FormActivity" />
        <activity android:name=".Social.RequestActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Social.RequestLogActivity" />
        </activity>
        <activity android:name=".Social.RequestLogActivity" />
        <activity android:name=".messages.ChatLogActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".messages.MessagesActivity" />
        </activity>
        <activity android:name=".messages.NewMessageActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".messages.MessagesActivity" />
        </activity>
        <activity android:name=".messages.MessagesActivity" />
        <activity android:name=".registerlogin.MainActivity" />
        <activity android:name=".registerlogin.RegisterActivity" />
    </application>

</manifest>