<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.erasmusfamily">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_icona_app_no_scritta_def_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_icona_app_no_scritta_def"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Social.CheckDocumentActivity">

        </activity>
        <activity android:name=".Social.MeteViewActivity"></activity>
        <activity
            android:name=".Setting.ChangePasswordActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Setting.SettingActivity" />
        </activity>
        <activity
            android:name=".Setting.SettingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Social.FormLogActivity"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>
        <activity
            android:name=".Social.FormActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Social.RequestActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Social.RequestLogActivity" />
        </activity>
        <activity
            android:name=".Social.RequestLogActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".messages.ChatLogActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".messages.MessagesActivity" />
        </activity>
        <activity
            android:name=".messages.NewMessageActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".messages.MessagesActivity" />
        </activity>
        <activity
            android:name=".messages.MessagesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".registerlogin.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".registerlogin.RegisterActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>